{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "parserOptions": {
        "ecmaVersion": 2020
    },

    "env": {
        "es6": true
    },
    "rules": {
      "react/jsx-filename-extension": 0,
      "import/prefer-default-export": 0,
      "react/button-has-type": 0,
      "react/jsx-props-no-spreading": 0,
      "react/forbid-prop-types": 0,
      "max-len": 0
    },
    "settings": {
      "import/resolver": {
        "node": {
          "paths": [
            "src"
          ]
        }
      }
    }
}